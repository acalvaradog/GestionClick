<script>

    document.getElementById("VacacionesPagadas").addEventListener("input", VacacionesPagas);


    var cantdiaspendi = 0;
    $("#Btn_AutorVac").hide();
    var NumeroEmpleado = $("#EmpleadoId").val();
    $("#AutorizaVacPag").hide();
    $("#AutorizaVacAdel").hide();
    $("#DiasMayora6").hide();
    var Empl = $("#EmpleadoId").val();


