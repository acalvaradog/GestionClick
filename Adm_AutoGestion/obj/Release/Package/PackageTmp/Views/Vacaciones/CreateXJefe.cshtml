                        $("#AutorizaVacPag").hide();
                        $("#AutorizaVacAdel").show();
                        $("#DiasMayora6").show();
                    }
                    if (data == "DsMayorPag") {
                        $("#AutorizaVacPag").show();
                        $("#AutorizaVacAdel").hide();
                        $("#DiasMayora6").show();
                    }
                    if (data == "AntPag") {
                        $("#AutorizaVacPag").show();
                        $("#AutorizaVacAdel").show();
