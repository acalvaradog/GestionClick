    function cambiarFechaMaxima() {
        var FI = new Date(document.getElementById('FechaInicio').value);
        var FF = document.getElementById('FechaFin');

        //var dd = FI.getUTCDate();
        //var mm = FI.getUTCMonth() + 1;
        var yyyy = FI.getUTCFullYear();

        //if (dd < 10) {
        //    dd = '0' + dd;
        //}

        //if (mm < 10) {
        //    mm = '0' + mm;
        //}
        FF.disabled = false;
        FF.value = "";
        var fechaMin = yyyy + '-' + '01' + '-' + '01';
        /*yyyy = FI.getFullYear() + 1;*/
        var fechaMax = yyyy + '-' + 12 + '-' + 31;
        FF.setAttribute("max", fechaMax);
        FF.setAttribute("min", fechaMin);
    }

    function alternar() {
        var A = document.getElementById('Area');
        var C = document.getElementById('Cargo');

        if (A.value != "") {
            C.disabled = true;
            C.value = "";
        } else if (C.value != "") {
            A.disabled = true;
            A.value = "";
        } else if (A.value == "") {
            C.disabled = false;
            C.value = "";
            A.disabled = false;
            A.value = "";
        } else if (C.value == "") {
            A.disabled = false;
            A.value = "";
            C.disabled = false;
            C.value = "";
        }

    }
</script>

