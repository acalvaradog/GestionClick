                    <form>
                        <div class="row">
                            <div class="col-6">
                                <label>Fecha Inicio</label>
                                <input class="form-control" type="date" id="FechaInicio" name="FechaInicio" onblur="cambiarFechaMaxima();" required />
                            </div>

                            <div class="col-6">
                                <label>Fecha Fin</label>
                                <input class="form-control" type="date" id="FechaFin" name="FechaFin" disabled required />
                            </div>
                        </div>

                        <div class="row mt-2">
                            <div class="col-6">
                                <label>Area</label>
                                @Html.DropDownList("Area", new SelectList(ViewBag.Areas, "Key", "Key"), "Seleccione...", htmlAttributes: new { @class = "form-control selectpicker", data_live_search = "true", onchange = "javascript: alternar();" })
                            </div>

                            <div class="col-6">
                                <label>Cargo</label>
                                @Html.DropDownList("Cargo", new SelectList(ViewBag.Cargos, "Key", "Key"), "Seleccione...", htmlAttributes: new { @class = "form-control selectpicker", data_live_search = "true", onchange = "javascript: alternar();" })
                            </div>
                        </div>

                        <div class="row mt-2">
                            <div class="col-12">
                                <button type="button" class="bi bi-filter-circle btn btn-outline-primary mb-2" onclick="javascript: filtrar();"> Filtrar</button>
