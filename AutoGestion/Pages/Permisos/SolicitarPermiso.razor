@page "/registrarpermiso"
@inject Blazored.LocalStorage.ILocalStorageService localStore
@using Autogestion.Shared.DTO.Empleado
@inject NavigationManager NavigationManager
<MudContainer MaxWidth="MaxWidth.ExtraExtraLarge">
    <iframe src="@Url" style="width:100%;height:750px;"></iframe>
</MudContainer>
@code {

    public string Url { get; set; }
    private EmpleadoReturnSapDTO datosempleado { get; set; }
    protected override async Task OnInitializedAsync()
    {
        var baseUri = NavigationManager.BaseUri;
        datosempleado = await localStore.GetItemAsync<EmpleadoReturnSapDTO>("User");
        Url = $"{baseUri}/permisos/permisos.html?id={datosempleado.Id}&codigo={datosempleado.PERNR}";
    }
}